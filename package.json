{
  "name": "grokjs",
  "version": "3.0.0-beta",
  "description": "Regular expression template library inspired by logstash grok filter module",
  "repository": "https://github.com/honzahommer/grokjs.git",
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "lint": "semistandard \"lib/**/*.js\" --fix",
    "unit": "mocha ./test/*.test.js",
    "test": "npm run lint && npm run unit"
  },
  "keywords": [
    "regex",
    "grok",
    "logstash"
  ],
  "author": "Andrey Chausenko",
  "contributors": [
    "Honza Hommer <honza@hommer.cz>"
  ],
  "license": "ISC",
  "husky": {
    "hooks": {
      "pre-push": "npm test"
    }
  },
  "semistandard": {
    "env": [
      "mocha"
    ]
  },
  "dependencies": {
    "async": "^2.6.1",
    "collections": "^5.1.7",
    "oniguruma": "^7.0.2"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "husky": "^1.3.1",
    "mocha": "^5.2.0",
    "nyc": "^13.1.0",
    "semistandard": "^13.0.1"
  }
}
